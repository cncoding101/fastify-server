{
  "name": "fastify-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "copy-html-and-css": "mkdir -p src/static/html src/static/css && cp -R src/wcs/html src/wcs/css src/static",
    "build-wcs": "tsc -p tsconfig-wcs.json && tsc-alias -p tsconfig-wcs.json && npm run copy-html-and-css",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "npm run build && node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/static": "^7.0.4",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@sinclair/typebox": "^0.32.29",
    "env-schema": "^5.2.1",
    "fastify": "^4.26.2",
    "fastify-swagger": "^5.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.10",
    "ajv": "^8.13.0",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.9",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
